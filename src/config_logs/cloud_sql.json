{
  "name": "cloud_sql",
  "displayName": "Cloud SQL",
  "rules": [
    {
      "sources": [
        {
          "sourceType": "logs",
          "source": "resourceType",
          "condition": "$eq('cloudsql_database')"
        }
      ],
      "attributes": [
        {
          "key": "gcp.instance.id",
          "pattern": "resource.labels.database_id"
        }
      ]
    }
  ]
}