{
  "name": "transfer_service_agent",
  "displayName": "Google Storage Transfer agent",
  "rules": [
    {
      "sources": [
        {
          "sourceType": "logs",
          "source": "resourceType",
          "condition": "$prefix(cloud.gcp.storagetransfer_googleapis_com.agent)"
        },
        {
          "sourceType": "logs",
          "source": "resourceType",
          "condition": "$prefix(cloud.gcp.logging_googleapis_com)"
        }
      ],
      "attributes": [
        {
          "key": "agent_id",
          "pattern": "gcp.instance.id"
        },
        {
          "key": "project_id",
          "pattern": "gcp.project.id"
        },
        {
          "key": "agent_pool",
          "pattern": "agent_pool"
        }
      ]
    }
  ]
}