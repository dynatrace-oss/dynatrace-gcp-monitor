{
    "name": "storage_transfer_job",
    "displayName": "Google Storage Transfer job",
    "rules": [
        {
            "sources": [
                {
                    "sourceType": "logs",
                    "source": "resourceType",
                    "condition": "$prefix(cloud.gcp.storagetransfer_googleapis_com.transferjob)"
                },
                {
                    "sourceType": "logs",
                    "source": "resourceType",
                    "condition": "$prefix(cloud.gcp.logging_googleapis_com)"
                }
            ],
            "attributes": [
                {
                    "key": "job_id",
                    "pattern": "gcp.instance.id"
                },
                {
                    "key": "project_id",
                    "pattern": "gcp.project.id"
                }
            ]
        }
    ]
}