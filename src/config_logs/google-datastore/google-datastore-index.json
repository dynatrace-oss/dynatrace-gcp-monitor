{
    "name": "datastore_index",
    "displayName": "Datastore Index",
    "rules": [
        {
            "sources": [
                {
                    "sourceType": "logs",
                    "source": "resourceType",
                    "condition": "$eq('datastore_index')"
                }
            ],
            "attributes": [
                {
                    "key": "content",
                    "pattern": "@"
                },
                {
                    "key": "database_id",
                    "pattern": "resource.labels.database_id"
                },
                {
                    "key": "index_id",
                    "pattern": "resource.labels.index_id"
                },
                {
                    "key": "project_id",
                    "pattern": "resource.labels.project_id"
                }
            ]
        }
    ]
}