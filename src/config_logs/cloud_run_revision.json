{
  "name": "cloud_run_revision",
  "displayName": "Cloud Run",
  "rules": [
    {
      "sources": [
        {
          "sourceType": "logs",
          "source": "resourceType",
          "condition": "$eq('cloud_run_revision')"
        }
      ],
      "attributes": [
        {
          "key": "gcp.instance.name",
          "pattern": "resource.labels.revision_name"
        },
        {
          "key": "content",
          "pattern": "@"
        }
      ]
    }
  ]
}
