{
    "displayName": "dynatrace-gcp-monitor Self monitoring",
    "gridLayout": {
      "columns": "2",
      "widgets": [
        {
          "title": "MINT lines ingested",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/dynatrace/ingest_lines\" resource.type=\"generic_task\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_MEAN"
                    },
                    "secondaryAggregation": {}
                  }
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Dynatrace connectivity",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/dynatrace/connectivity\" resource.type=\"generic_task\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_MEAN"
                    },
                    "secondaryAggregation": {}
                  }
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Function execution time",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"cloudfunctions.googleapis.com/function/execution_times\" resource.type=\"cloud_function\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_PERCENTILE_99"
                    },
                    "secondaryAggregation": {}
                  },
                  "unitOverride": "ns"
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Execution time by phase",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/dynatrace/phase_execution_time\" resource.type=\"generic_task\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "metric.label.\"status\"",
                        "metric.label.\"function_name\"",
                        "metric.label.\"phase\""
                      ]
                    },
                    "secondaryAggregation": {}
                  }
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Dynatrace requests count",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/dynatrace/request_count\" resource.type=\"generic_task\" metric.label.\"response_code\"=\"202\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "metric.label.\"dynatrace_tenant_url\""
                      ]
                    },
                    "secondaryAggregation": {}
                  }
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Dynatrace failed requests count",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"custom.googleapis.com/dynatrace/request_count\" resource.type=\"generic_task\" metric.label.\"response_code\"=\"400\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_MEAN",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "metric.label.\"dynatrace_tenant_url\""
                      ]
                    },
                    "secondaryAggregation": {}
                  }
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Function memory used",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"cloudfunctions.googleapis.com/function/user_memory_bytes\" resource.type=\"cloud_function\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_PERCENTILE_99"
                    },
                    "secondaryAggregation": {}
                  },
                  "unitOverride": "By"
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        },
        {
          "title": "Function executions",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"cloudfunctions.googleapis.com/function/execution_count\" resource.type=\"cloud_function\"",
                    "aggregation": {
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "resource.label.\"region\""
                      ]
                    },
                    "secondaryAggregation": {}
                  },
                  "unitOverride": "1"
                },
                "plotType": "LINE",
                "minAlignmentPeriod": "60s"
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
            },
            "chartOptions": {
              "mode": "COLOR"
            }
          }
        }
      ]
    }
}