{
    "id": "cloud.gcp.file_googleapis_com.nfs.server.free_bytes_percent_alert",
    "enabled": true,
    "name": "Google Filestore Instance free disk space percent [GCP]",
    "description": "The {metricname} value of {severity} was {alert_condition} your custom threshold of {threshold}.",
    "metricId": "cloud.gcp.file_googleapis_com.nfs.server.free_bytes_percent",
    "aggregationType": "AVG",
    "severity": "RESOURCE_CONTENTION",
    "monitoringStrategy": {
        "type": "STATIC_THRESHOLD",
        "alertCondition": "BELOW",
        "samples": 10,
        "dealertingSamples": 10,
        "violatingSamples": 3,
        "threshold": 5,
        "unit": "PERCENT"
    }
}